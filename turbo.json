{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:transcription": {
      "cache": false,
      "dependsOn": ["@poll-automation/types#build"]
    },
    "dev:web": {
      "cache": false
    },
    "lint": {},
    "format": {},
    "test": {}
  }
}
